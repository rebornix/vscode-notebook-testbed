{
  "name": "vscode-notebook-testbed",
	"displayName": "vscode-notebook-testbed",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.53.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
        "onNotebook:mimetype-test",
		"onCommand:testbed.streamoutput",
		"onCommand:testbed.print"
	],
	"main": "./out/extension.js",
	"enableProposedApi": true,
	"contributes": {
		"commands": [
			{
				"command": "testbed.streamoutput",
				"title": "NB Test: Stream Output"
			},
			{
				"command": "testbed.print",
				"title": "NB Test: Print"
			}
		],
		"notebookProvider": [
			{
				"viewType": "mimetype-test",
				"displayName": "MimeType Test",
				"selector": [
					{
						"filenamePattern": "*.mttnb"
					}
				]
			}
		],
		"notebookOutputRenderer": [
			{
				"id": "mimetyperenderer",
				"displayName": "Mimtype Renderer",
				"mimeTypes": [
					"text/markdown"
				],
				"entrypoint": "./renderer/index.js"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js",
		"update-proposed-api": "vscode-dts dev"
	},
	"devDependencies": {
		"@types/vscode": "^1.53.0",
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"eslint": "^7.15.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1",
		"vscode-dts": "^0.3.1"
	}
}
